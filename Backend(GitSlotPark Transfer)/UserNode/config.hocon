akka {
	loggers = ["Akka.Logger.NLog.NLogLogger, Akka.Logger.NLog"]
	actor {
		provider = cluster
		deployment {            								
			/slotgameRouter {
				router = round-robin-group
				routees.paths = ["/user/gameManager"]
				cluster {
					enabled = on
					allow-local-routees = off
					use-role = slotgame
				}
			}
			/redisWriter {
				router 			= consistent-hashing-pool
				nr-of-instances = 5
			}
			/httpWorkers {
				router 			= consistent-hashing-pool
				nr-of-instances = 20
			}
			/httpAuthWorkers {
				router 			= consistent-hashing-pool
				nr-of-instances = 20
			}
			/httpReplayWorkers {
				router 			= consistent-hashing-pool
				nr-of-instances = 20			
			}
			/httpHistoryWorkers {
				router 			= consistent-hashing-pool
				nr-of-instances = 20						
			}
			/apiWorkers {
				router 			= consistent-hashing-pool
				nr-of-instances = 10										
			}
		}
	}
						
	remote {
		dot-netty.tcp {
				hostname = "127.0.0.1"
				port     = 8083
				maximum-frame-size = 2048000b
			}
	}		

	cluster {
		seed-nodes = ["akka.tcp://godgaming@127.0.0.1:4055"]
		roles = ["user"]
	}
}